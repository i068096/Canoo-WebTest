<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<HTML>
    <HEAD>
        <TITLE>Form with JavaScript that changes form properties</TITLE>
    </HEAD>

    <BODY >
        <form name="selectTableForm" method="POST" action="formTest" target="_blank">
            <input type="hidden" name="firstHidden" value="firstHidden">

            <select name="chosenId" size="3" multiple>
                <option value="0">One</option>
                <option value="1">Two</option>
                <option value="2">Three</option>
            </select>
            <p>
                <input type="submit" name="selector_chosenId" value="Select" onClick="document.selectTableForm.action='formTest';document.selectTableForm.target=''">
            </p>
            <select name="appLink" size="3">
                <option value="LNK_0" selected>App Zero</option>
                <option value="LNK_1">App One</option>
                <option value="LNK_2">App Two</option>
            </select>
            <p>
                <input type="submit" name="SingleSubmit" value="Show" onClick="document.selectTableForm.action='formTest?action=more';document.selectTableForm.target='_blank'">
            </p>
        </form>
    </BODY>
</HTML>
